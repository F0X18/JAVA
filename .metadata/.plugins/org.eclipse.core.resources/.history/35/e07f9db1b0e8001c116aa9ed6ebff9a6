	public class Estudiante()
		
	
	public Estudiante() {
		this.id=1;
		this.edad=18;
		this.telefono=999999999;
		this.numeroDeNotas=10;
		this.notaMedia=10;
	}
	public Estudiante(int id){
		this.id=id;
		this.edad=20;
		this.telefono=666666666;
		this.numeroDeNotas=8;
		this.notaMedia=6.5;
	}
	public Estudiante(float notaMedia, int numeroDeNotas) {
		int id;
		byte edad;
		int telefono;
		int numeroDeNotas;
		float notaMedia;
		
	}
	//atributos
	int id;
	byte edad;
	int telefono;
	int numeroDeNotas;
	float notaMedia;
	
	//metodo
	void mostrarInfo();
	void agregarNuevaNota();
	
	
