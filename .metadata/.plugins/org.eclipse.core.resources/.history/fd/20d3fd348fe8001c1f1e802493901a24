
public class numerosPrimos {

	public static void main(String[] args) {
		int numeroPrimos=3;
		boolean primoSiNo=true;
		for(int i=numeroPrimos; i>1; i--){
		if(numeroPrimos%i ==0) {
			primoSiNo=false;
			
			}else{
				
			primoSiNo=true;
			}
		}	
			
			
			
			
		if((numeroPrimos>1) && (numeroPrimos/numeroPrimos==1)&&(numeroPrimos/1==numeroPrimos)&&(numeroPrimos));
		}
		System.out.printf("el %d es primo o <=  1").println();
		}else {
		System.out.printf("el %d no es primo").println();
		}
	}

}
